          \u0441 \u043D\u0438\u043C."
        Product: "\u043A\u0430\u043C\u0435\u043D\u044C"
      - Id: mushroom
        Title: "\u0413\u0440\u0438\u0431\u043E\u0447\u0435\u043A"
        Action: "\u0421\u043E\u0431\u0440\u0430\u0442\u044C \u0433\u0440\u0438\u0431"
        Description: "\u0413\u0440\u0438\u0431 \u043A\u043E\u0442\u043E\u0440\u044B\u0439
          \u043C\u043E\u0436\u043D\u043E \u0441\u044A\u0435\u0441\u0442\u044C \u043C\u0438\u043D\u0438\u043C\u0443\u043C
          \u043E\u0434\u0438\u043D \u0440\u0430\u0437"
        Product: "\u0433\u0440\u0438\u0431"
      - Id: berry_bush
        Title: "\u042F\u0433\u043E\u0434\u043D\u044B\u0439 \u043A\u0443\u0441\u0442"
        Action: "\u0421\u043E\u0431\u0440\u0430\u0442\u044C \u044F\u0433\u043E\u0434\u0443"
        Description: "\u041A\u0443\u0441\u0442 \u0441 \u0432\u043F\u043E\u043B\u043D\u0435
          \u0441\u044A\u0435\u0434\u043E\u0431\u043D\u044B\u043C\u0438 (\u043D\u0430\u0434\u043E
          \u043F\u0440\u043E\u0432\u0435\u0440\u0438\u0442\u044C) \u044F\u0433\u043E\u0434\u0430\u043C\u0438.
          \u042F\u0433\u043E\u0434\u044B \u043A\u0440\u0430\u0441\u043D\u043E\u0433\u043E
          \u0446\u0432\u0435\u0442\u0430, \u043F\u043E\u0445\u043E\u0436\u0438\u0435
          \u043D\u0430 \u0432\u043E\u043B\u0447\u044C\u0438."
        Product: "\u044F\u0433\u043E\u0434\u0430"
      - Id: berry
        Title: "\u042F\u0433\u043E\u0434a"
        Action: "\u041F\u043E\u0434\u043D\u044F\u0442\u044C \u044F\u0433\u043E\u0434\u0443"
        Description: "\u042F\u0433\u043E\u0434\u0430 \u043A\u0440\u0430\u0441\u043D\u0430\u044F,
          \u0432\u044B\u0433\u043B\u044F\u0434\u0438\u0442 \u0441\u044A\u0435\u0434\u043E\u0431\u043D\u043E\u0439"
        Product: "\u044F\u0433\u043E\u0434\u0430"
      - Id: twig
